const convert = (str) => {
  for (var x = 0; x < str.length; x++) {
    if (['a', 'b', 'c', 'd', 'e', 'f', 'g'].includes(str[x]) {
      //note
    } else if (str[x].match(/[!.?]/)) {
      //rest
    } else {
      //duration
    }
  }
}


